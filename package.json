{
	"name": "@webamoki/web-svelte",
	"publishConfig": {
		"access": "public"
	},
	"version": "0.5.27",
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && pnpm prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "pnpm fmt && pnpm lint && pnpm test",
		"fmt": "prettier --write --cache --cache-location=.cache/.prettier-cache --cache-strategy metadata --log-level warn .",
		"lint": "pnpm lint:es && pnpm lint:svelte",
		"lint:es": "eslint --cache --cache-location .cache/.eslintcache --cache-strategy metadata .",
		"lint:svelte": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"test:unit": "vitest run",
		"test": "pnpm test:unit",
		"watch": "svelte-package --watch"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"type": "module",
	"exports": {
		"./components/form": {
			"types": "./dist/components/form/index.d.ts",
			"svelte": "./dist/components/form/index.js"
		},
		"./components/ui": {
			"types": "./dist/components/ui/index.d.ts",
			"svelte": "./dist/components/ui/index.js"
		},
		"./utils/datetime": {
			"types": "./dist/utils/datetime/index.d.ts",
			"import": "./dist/utils/datetime/index.js"
		},
		"./utils/types/arktype": {
			"types": "./dist/utils/types/arktype.d.ts",
			"import": "./dist/utils/types/arktype.js"
		},
		"./utils/types/consts": {
			"types": "./dist/utils/types/consts.d.ts",
			"import": "./dist/utils/types/consts.js"
		},
		"./utils/types/db": {
			"types": "./dist/utils/types/db.d.ts",
			"import": "./dist/utils/types/db.js"
		},
		"./utils/form": {
			"types": "./dist/utils/form.d.ts",
			"import": "./dist/utils/form.js"
		},
		"./utils/search": {
			"types": "./dist/utils/search.d.ts",
			"import": "./dist/utils/search.js"
		}
	},
	"peerDependencies": {
		"@internationalized/date": "^3.9.0",
		"@lucide/svelte": "^0.544.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"arktype": "^2.1.22",
		"ramda": "^0.31.3",
		"svelte": "^5.0.0",
		"sveltekit-superforms": "^2.27.1",
		"tailwindcss": "^4.0.0"
	},
	"devDependencies": {
		"@changesets/cli": "^2.29.7",
		"@eslint/compat": "^1.4.1",
		"@eslint/js": "^9.39.1",
		"@lucide/svelte": "^0.544.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.48.4",
		"@sveltejs/package": "^2.5.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.17",
		"@types/node": "^22.19.0",
		"@types/ramda": "^0.31.1",
		"@types/sorted-array-functions": "^1.3.3",
		"arktype": "^2.1.25",
		"bits-ui": "^2.14.2",
		"clsx": "^2.1.1",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.0",
		"globals": "^16.5.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"publint": "^0.3.15",
		"shiki": "^3.15.0",
		"svelte": "^5.43.4",
		"svelte-check": "^4.3.3",
		"sveltekit-superforms": "^2.28.1",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^3.1.1",
		"tailwindcss": "^4.1.17",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.3",
		"vite": "^7.2.1",
		"vitest": "^3.2.4"
	},
	"keywords": [
		"svelte"
	],
	"pnpm": {
		"onlyBuiltDependencies": [
			"@tailwindcss/oxide",
			"esbuild"
		]
	},
	"dependencies": {
		"dotenv": "^17.2.3",
		"drizzle-orm": "^0.44.7",
		"formsnap": "^2.0.1",
		"pg": "^8.16.3",
		"ramda": "^0.31.3",
		"sorted-array-functions": "^1.3.0",
		"svelte-awesome-color-picker": "^4.1.0",
		"svelte-sonner": "^1.0.5"
	}
}
