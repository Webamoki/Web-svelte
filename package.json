{
	"name": "@webamoki/web-svelte",
	"publishConfig": {
		"access": "public"
	},
	"version": "0.5.1",
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && pnpm prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "pnpm fmt && pnpm lint && pnpm test",
		"fmt": "prettier --write --cache --cache-location=.cache/.prettier-cache --cache-strategy metadata --log-level warn .",
		"lint": "pnpm lint:es && pnpm lint:svelte",
		"lint:es": "eslint --cache --cache-location .cache/.eslintcache --cache-strategy metadata .",
		"lint:svelte": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"test:unit": "vitest run",
		"test": "pnpm test:unit"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"type": "module",
	"exports": {
		"./components": {
			"types": "./dist/components/index.d.ts",
			"svelte": "./dist/components/index.js"
		},
		"./utils": {
			"types": "./dist/utils/index.d.ts",
			"import": "./dist/utils/index.js"
		}
	},
	"peerDependencies": {
		"@internationalized/date": "^3.9.0",
		"@lucide/svelte": "^0.544.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"arktype": "^2.1.22",
		"ramda": "^0.31.3",
		"svelte": "^5.0.0",
		"sveltekit-superforms": "^2.27.1",
		"tailwindcss": "^4.0.0"
	},
	"devDependencies": {
		"@changesets/cli": "^2.29.7",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.22.0",
		"@internationalized/date": "^3.9.0",
		"@lucide/svelte": "^0.544.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@types/node": "^22",
		"@types/ramda": "^0.31.1",
		"@types/sorted-array-functions": "^1.3.3",
		"arktype": "^2.1.22",
		"clsx": "^2.1.1",
		"eslint": "^9.22.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"publint": "^0.3.2",
		"shiki": "^3.13.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"sveltekit-superforms": "^2.27.1",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^3.1.1",
		"tailwindcss": "^4.0.0",
		"tw-animate-css": "^1.3.8",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.4",
		"vitest": "^3.2.3"
	},
	"keywords": [
		"svelte"
	],
	"pnpm": {
		"onlyBuiltDependencies": [
			"@tailwindcss/oxide",
			"esbuild"
		]
	},
	"dependencies": {
		"dotenv": "^17.2.3",
		"drizzle-orm": "^0.44.5",
		"formsnap": "^2.0.1",
		"pg": "^8.16.3",
		"ramda": "^0.31.3",
		"sorted-array-functions": "^1.3.0",
		"svelte-awesome-color-picker": "^4.0.2",
		"svelte-sonner": "^1.0.5"
	}
}
